{
    "version": "0.2.0",
    "configurations": [
    {
        "name": "PowerShell Launch Current File",
        "type": "PowerShell",
        "request": "launch",
        "script": "${file}",
        "cwd": "${workspaceFolder}"
    },
    {
        "name": ".NET Core Attach",
        "type": "coreclr",
        "request": "attach"
    },
        {
            "name": "Launch and Debug Standalone Blazor WebAssembly App",
            "type": "blazorwasm",
            "request": "launch",
            "cwd": "${workspaceFolder}/src/BlazorGolfClient"
        }
    ]
}